<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <WebView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/webview">
    </WebView>

    <RelativeLayout android:layout_width="match_parent"
        android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".Home">

        <ImageView
            android:id="@+id/imgCima"
            android:src="@drawable/cima"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_above="@+id/imgBaixo"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageView
            android:id="@+id/imgBaixo"
            android:src="@drawable/baixo"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageView
            android:id="@+id/imgEsquerda"
            android:src="@drawable/esquerda"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_alignTop="@+id/imgDireita"
            android:layout_toLeftOf="@+id/imgDireita"
            android:layout_toStartOf="@+id/imgDireita" />

        <ImageView
            android:id="@+id/imgDireita"
            android:src="@drawable/direita"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_alignTop="@+id/imgBaixo"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />
    </RelativeLayout>
</FrameLayout>